{
    "name": "Slack_alert_job_01",
    "email_notifications": {
        "on_failure": [],
        "no_alert_for_skipped_runs": false
    },
    "webhook_notifications": {},
    "timeout_seconds": 0,
    "schedule": {
        "quartz_cron_expression": "43 0 15 * * ?",
        "timezone_id": "America/Los_Angeles",
        "pause_status": "UNPAUSED"
    },
    "max_concurrent_runs": 1,
    "tasks": [
        {
            "task_key": "Slack_alert_task",
            "run_if": "ALL_SUCCESS",
            "notebook_task": {
                "notebook_path": "/Shared/TEST_INTEGRATION/TEST_CONN_01/slack_msg",
                "source": "WORKSPACE"
            },
            "job_cluster_key": "Slack_alert_cluster",
            "libraries": [
                {
                    "pypi": {
                        "package": "google-api-python-client"
                    }
                }
            ],
            "max_retries": 1,
            "min_retry_interval_millis": 3600000,
            "retry_on_timeout": false,
            "timeout_seconds": 1200,
            "email_notifications": {}
        }
    ],
    "job_clusters": [
        {
            "job_cluster_key": "Slack_alert_cluster",
            "new_cluster": {
                "cluster_name": "",
                "spark_version": "10.4.x-scala2.12",
                "spark_conf": {
                    "spark.master": "local[*, 4]",
                    "spark.databricks.cluster.profile": "singleNode"
                },
                "aws_attributes": {
                    "first_on_demand": 1,
                    "availability": "SPOT_WITH_FALLBACK",
                    "zone_id": "us-east-1b",
                    "spot_bid_price_percent": 100,
                    "ebs_volume_type": "GENERAL_PURPOSE_SSD",
                    "ebs_volume_count": 3,
                    "ebs_volume_size": 100
                },
                "node_type_id": "m4.2xlarge",
                "driver_node_type_id": "m4.2xlarge",
                "custom_tags": {
                    "ResourceClass": "SingleNode"
                },
                "enable_elastic_disk": false,
                "data_security_mode": "LEGACY_SINGLE_USER_STANDARD",
                "runtime_engine": "STANDARD",
                "num_workers": 0
            }
        }
    ],
    "format": "MULTI_TASK"
}